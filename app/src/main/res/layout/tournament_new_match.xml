<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    style="@style/screen_layout_style"
    tools:context="com.naveen.mytennisstats.NewMatchActivity">



    <include layout="@layout/new_match_reuse"
        android:layout_height="316dp" />

    <EditText
        style="@style/text_view_style"
        android:hint="What's the Final score"
        android:id="@+id/editTexttournamentFinalScore"
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content" />


    <EditText
        style="@style/text_view_style"
        android:id="@+id/editTextTournamentNewMatchComments"
        android:layout_width="wrap_content"
        android:hint="Notes"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content" />


    <EditText
        style="@style/text_view_style"
        android:id="@+id/editTextTournamentNewMatchName"
        android:layout_width="wrap_content"
        android:hint="What's the Match name"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content" />


    </LinearLayout>
